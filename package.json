		"@typescript-eslint/parser": "^6.21.0",
		"cz-conventional-changelog": "^3.3.0",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-svelte": "^2.30.0",
		"prettier": "^3.2.5",
		"prettier-plugin-svelte": "^3.0.0",
		"prisma": "^5.9.1",
		"sass": "^1.70.0",
